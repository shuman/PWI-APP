@charset "UTF-8";

@font-face {
  font-family: "pwi-font";
  src:url("fonts/pwi-font.eot");
  src:url("fonts/pwi-font.eot?#iefix") format("embedded-opentype"),
    url("fonts/pwi-font.woff") format("woff"),
    url("fonts/pwi-font.ttf") format("truetype"),
    url("fonts/pwi-font.svg#pwi-font") format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "pwi-font" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="pwi-"]:before,
[class*=" pwi-"]:before {
  font-family: "pwi-font" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

